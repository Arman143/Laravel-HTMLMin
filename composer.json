{
    "name": "graham-campbell/htmlmin",
    "description": "HTMLMin Is A Simple HTML Minifier For Laravel 4",
    "keywords": ["laravel", "framework", "html minifier", "html", "minifier", "HTMLMin", "Laravel HTMLMin", "Laravel-HTMLMin", "Graham Campbell", "GrahamCampbell"],
    "license": "Apache License 2.0",
    "authors": [
        {
            "name": "Graham Campbell",
            "email": "graham@mineuk.com"
        }
    ],
    "require": {
        "php": ">=5.3.3",
        "orchestra/testbench": "2.0.*|2.1.*",
        "mrclay/minify": "2.1.*"
    },
    "require-dev": {
        "mockery/mockery": "0.8.*",
        "phpunit/phpunit": "3.7.*"
    },
    "autoload": {
        "psr-0": {
            "GrahamCampbell\\HTMLMin": "src/"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "0.3-dev"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
