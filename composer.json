{
    "name": "graham-campbell/htmlmin",
    "description": "HTMLMin Is A Simple HTML Minifier For Laravel 4.1",
    "keywords": ["laravel", "framework", "html minifier", "html", "minifier", "HTMLMin", "Laravel HTMLMin", "Laravel-HTMLMin", "Graham Campbell", "GrahamCampbell"],
    "license": "Apache License 2.0",
    "authors": [
        {
            "name": "Graham Campbell",
            "email": "graham@mineuk.com"
        }
    ],
    "require": {
        "php": ">=5.4.7",
        "laravel/framework": "4.1.*",
        "mrclay/minify": "2.1.*"
    },
    "require-dev": {
        "graham-campbell/testbench": "0.1.*"
    },
    "autoload": {
        "psr-0": {
            "GrahamCampbell\\HTMLMin\\Classes": "src/",
            "GrahamCampbell\\HTMLMin\\Facades": "src/",
            "GrahamCampbell\\HTMLMin\\Support": "src/",
            "GrahamCampbell\\HTMLMin\\Tests": "tests/"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-develop": "0.4-dev"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
